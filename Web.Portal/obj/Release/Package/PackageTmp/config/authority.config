<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE collegeAuthor>
<collegeAuthor>

  <!--************************新闻中心权限配置开始***************************-->
  <!--新闻栏目管理-->
  <container areaName="" controllerName="news" actionName="NewsTypeMgtResult">
    <!--展示页面-->
    <subrights  controllerName="news" actionName="NewsTypeMgtResult"/>
    <!--修改新闻栏目信息-->
    <subrights  controllerName="news" actionName="modifyNewsType"/>
    <!--获得新闻栏目列表-->
    <subrights  controllerName="news" actionName="GetNewsTypeList"/>
    <!--删除新闻栏目-->
    <subrights  controllerName="news" actionName="DeleteNewsType"/>
    <!--批量删除新闻栏目-->
    <subrights  controllerName="news" actionName="DeleteNewsTypeBatch"/>
  </container>
  <!--新闻管理-->
  <container areaName="" controllerName="news" actionName="NewsInfoMgtResult">
    <!--展示页面-->
    <subrights  controllerName="news" actionName="NewsInfoMgtResult"/>
    <!--获得新闻列表-->
    <subrights  controllerName="news" actionName="GetNewsInfoList"/>
    <!--获得新闻列表-->
    <subrights  controllerName="news" actionName="AddNewsInfo"/>
    <!--发布新闻-->
    <subrights  controllerName="news" actionName="PubNewsInfo"/>
    <!--取消发布新闻-->
    <subrights  controllerName="news" actionName="UnPubNewsInfo"/>
    <!--删除新闻-->
    <subrights  controllerName="news" actionName="DeleteNewsInfo"/>
    <!--获得单个新闻详情-->
    <subrights  controllerName="news" actionName="GetNewsInfoDetail"/>
    <!--获得编辑单个新闻详情-->
    <subrights  controllerName="news" actionName="modifyNewsInfo"/>
    <!--上传图片-->
    <subrights  controllerName="news" actionName="upload"/>
    <!-- 设置缩略图-->
    <subrights controllerName="news" actionName="SetNewsListImg"/>
  </container>

  <!--************************新闻中心权限配置结束***************************-->

  <!--************************系统设置权限配置开始***************************-->
  <!--用户管理-->
  <container areaName="" controllerName="system" actionName="SysUserMgtResult">
    <!--展示页面-->
    <subrights  controllerName="system" actionName="SysUserMgtResult"/>
    <!--修改用户信息-->
    <subrights  controllerName="system" actionName="ModifyUserInfo"/>
    <!--删除用户-->
    <subrights  controllerName="system" actionName="DeleteUser"/>
    <!--获得用户列表-->
    <subrights  controllerName="system" actionName="GetALlUserList"/>
  </container>

  <!--角色管理-->
  <container areaName="" controllerName="system" actionName="SysRoleMgtResult">
    <!--展示页面-->
    <subrights controllerName="system" actionName="SysRoleMgtResult"/>
    <!--获得角色列表-->
    <subrights controllerName="system" actionName="GetRoleList"/>
    <!--获得角色键值对列表-->
    <subrights controllerName="system" actionName="GetRoleItemList"/>
    <!--修改角色-->
    <subrights controllerName="system" actionName="ModifyRoleInfo"/>
    <!--获得角色树-->
    <subrights controllerName="system" actionName="GetFuncTree"/>
    <!--获得所有非租户权限树-->
    <subrights controllerName="system" actionName="GetAllNotTenantFuncsTree"/>
    <!--删除角色-->
    <subrights controllerName="system" actionName="DeleteRole"/>
    <!--获得角色树-->
    <subrights controllerName="system" actionName="GetRoleSelect2List"/>
  </container>

  <!--租户管理-->
  <container areaName="" controllerName="system" actionName="SysTenantMgtResult">
    <!--展示页面-->
    <subrights controllerName="system" actionName="SysTenantMgtResult"/>
    <!--获得租户列表-->
    <subrights controllerName="system" actionName="GetTenantList"/>
    <!--修改租户信息-->
    <subrights controllerName="system" actionName="ModifyTenantInfo"/>
    <!--删除租户信息-->
    <subrights controllerName="system" actionName="DeleteTenantInfo"/>
    <!-- 设置ioc和logo-->
    <subrights controllerName="system" actionName="SetTenantLogoOrIoc"/>
  </container>

  <!--项目配置-->
  <container areaName="" controllerName="system" actionName="ProjectSetting">
    <!--展示页面-->
    <subrights controllerName="system" actionName="ProjectSetting"/>
    <!--获得项目列表-->
    <subrights controllerName="system" actionName="GetAllProject"/>
    <!--设置推荐项目-->
    <subrights controllerName="system" actionName="Cancel"/>
    <!--取消推荐项目-->
    <subrights controllerName="system" actionName="SetProjetHost"/>
    <subrights controllerName="org" actionName="GetProjectItem"/>

  </container>
  <!--************************系统设置权限配置结束***************************-->


  <!--************************机构设置权限配置开始***************************-->
  <!--项目管理-->
  <container areaName="" controllerName="Org" actionName="ProjectMgtResul">
    <!--展示页面-->
    <subrights controllerName="Org" actionName="ProjectMgtResul"/>
    <!--新增项目信息-->
    <subrights controllerName="Org" actionName="ProjectInfo"/>
    <!--获得数据列表-->
    <subrights controllerName="Org" actionName="GetProjectPagedList"/>
    <!--新增项目-->
    <subrights controllerName="Org" actionName="AddProject"/>
    <!--删除项目-->
    <subrights controllerName="Org" actionName="DeleteItem"/>
    <!--获得项目类型列表-->
    <subrights controllerName="Org" actionName="GetProjectType"/>
    <!--提交审核-->
    <subrights controllerName="Org" actionName="SubmitAudit"/>
    <!--撤回审核-->
    <subrights controllerName="Org" actionName="WithdrawalAudit"/>
    <!--富文本上传图片-->
    <subrights  controllerName="news" actionName="upload"/>
  </container>

  <!--项目审核-->
  <container areaName="" controllerName="Project" actionName="ProjectList">
    <!--展示页面-->

    <subrights controllerName="Project" actionName="ProjectList"/>
    <!--审核通过-->
    <subrights controllerName="Org" actionName="AdoptStatus"/>
    <!--审核不通过-->
    <subrights controllerName="Org" actionName="DefeatedStatus"/>
    <!--管理员修改项目信息-->
    <subrights controllerName="Org" actionName="ModifyProjectInfo"/>
    <!--获得列表数据-->
    <subrights controllerName="Org" actionName="GetAuditProjectList"/>
    <!--编辑信息-->
    <subrights controllerName="Org" actionName="AdminProjectInfo"/>
    <!--保存审核信息-->
    <subrights controllerName="Org" actionName="SaveProject"/>
    <!--获得项目类别列表-->
    <subrights controllerName="Org" actionName="GetProjectType"/>
    <!--冻结和解冻-->
    <subrights  controllerName="Org" actionName="SetFreezeStatus"/>
    <!--富文本上传图片-->
    <subrights  controllerName="news" actionName="upload"/>
    <!--项目列表页面-->
    <subrights  controllerName="Project" actionName="GetProjectList"/>
    <!--冻结/解冻-->
    <subrights  controllerName="Project" actionName="SetFreezeStatus"/>
    <!--学员管理页面展示-->
    <subrights  controllerName="Project" actionName="StudentList"/>
    <!--获取学员列表-->
    <subrights  controllerName="Project" actionName="GetStudentList"/>
    <!-- 停课/复课-->
    <subrights  controllerName="Project" actionName="SetProjectStatus"/>
    <!--批量复课-->
    <subrights  controllerName="Project" actionName="RecoverStatusBatch"/>
    <!--批量停课-->
    <subrights  controllerName="Project" actionName="StopStatusBatch"/>
    <!--学员报名审批页面展示-->
    <subrights  controllerName="Project" actionName="ExamStudentList"/>
    <!--获取审核学员列表-->
    <subrights  controllerName="Project" actionName="GetExamStudentList"/>
    <!--审核通过/审核不通过-->
    <subrights  controllerName="Project" actionName="SetExamStudentStatus"/>
    <!--批量审核通过-->
    <subrights  controllerName="Project" actionName="ExamPassBatch"/>
    <!--批量审核不通过-->
    <subrights  controllerName="Project" actionName="ExamUnPassBatch"/>
    <!--证书管理页面-->
    <subrights  controllerName="Project" actionName="CertificateList"/>
    <!--获取证书管理列表-->
    <subrights  controllerName="Project" actionName="GetCertificateList"/>
    <!--标记为可领证-->
    <subrights  controllerName="Project" actionName="SetCertificateStatus"/>
    <!--批量标记为可领证-->
    <subrights  controllerName="Project" actionName="SetCertificateBatch"/>
  </container>

  <!--机构信息-->
  <container areaName="" controllerName="Org" actionName="SysOrgInfo">
    <!--展示页面-->
    <subrights controllerName="Org" actionName="SysOrgInfo"/>
    <!--展示页面-->
    <subrights controllerName="Org" actionName="UpdataPassword"/>
    <!--白名单设置-->
    <subrights controllerName="Org" actionName="SaveIpWhite"/>
    <!--更新租户密钥-->
    <subrights controllerName="Org" actionName="UpdateAesKey"/>
  </container>


  <!--数据统计权限配置开始-->
  <!--数据统计分析-->
  <container areaName="" controllerName="DataStat" actionName="AnalysisSummary">
    <!--展示页面-->
    <subrights controllerName="DataStat" actionName="AnalysisSummary"/>
    <!--拉取项目数统计接口-->
    <subrights controllerName="DataStat" actionName="GetTenantProjectStatdata"/>
    <!--拉取报名学员数统计接口-->
    <subrights controllerName="DataStat" actionName="GetTenantStudentEnterStatdata"/>
    <!--拉取各个合作机构数据汇总-->
    <subrights controllerName="DataStat" actionName="GetTenantSummarySatadata"/>
  </container>
  <!--报名情况统计分析-->
  <container areaName="" controllerName="DataStat" actionName="SignUpRecordReportStat">
    <!--展示页面-->
    <subrights controllerName="DataStat" actionName="SignUpRecordReportStat"/>
    <!--获取报名情况列表-->
    <subrights controllerName="DataStat" actionName="GetSignUpList"/>
  </container>
  <!--学习记录上报情况-->
  <container areaName="" controllerName="DataStat" actionName="LearnRecordReportStat">
    <!--展示页面-->
    <subrights controllerName="DataStat" actionName="LearnRecordReportStat"/>
    <!--获取学员学习记录集合-->
    <subrights controllerName="DataStat" actionName="GetLearnRrcords"/>
  </container>
  <!--考试记录上报情况-->
  <container areaName="" controllerName="DataStat" actionName="ExaminationReportStat">
    <!--展示页面-->
    <subrights controllerName="DataStat" actionName="ExaminationReportStat"/>
    <!--获取学员考试记录集合-->
    <subrights controllerName="DataStat" actionName="GetExamRrcords"/>
  </container>
  <!--缴费记录上报情况-->
  <container areaName="" controllerName="DataStat" actionName="PayReportStat">
    <!--展示页面-->
    <subrights controllerName="DataStat" actionName="PayReportStat"/>
    <!--获取学员缴费记录集合-->
    <subrights controllerName="DataStat" actionName="GetPayRrcords"/>
  </container>
  <!--数据统计权限配置结束-->
  
  <!--学员管理权限配置开始-->
  <container areaName="" controllerName="Student" actionName="StudentList">
    <!--展示页面-->
    <subrights controllerName="Student" actionName="StudentList"/>
    <!--获得合作机构-->
    <subrights controllerName="Student" actionName="GetTenantList"/>
    <!--获取合作机构的项目-->
    <subrights controllerName="Student" actionName="GetProjectListByTenant"/>
    <!--获取学员列表-->
    <subrights controllerName="Student" actionName="GetStudentList"/>
    <!--学员详细信息页-->
    <subrights controllerName="Student" actionName="StudentDetail"/>
    <!--学员信息编辑页面展示-->
    <subrights controllerName="Student" actionName="EditStudentInfo"/>
    <!--保存学员编辑信息-->
    <subrights controllerName="Student" actionName="EditStudentInfo"/>
    <!--学员学习记录页-->
    <subrights controllerName="Student" actionName="StudentLearnDetail"/>
    <!--获取学员的考试记录-->
    <subrights controllerName="Student" actionName="GetStudentExamList"/>
    <!--获取学员的缴费记录-->
    <subrights controllerName="Student" actionName="GetStudentPayList"/>
    <!--获取学员的学习记录-->
    <subrights controllerName="Student" actionName="GetStudentLearnList"/>
    <!--学员信息变更历史-->
    <subrights controllerName="Student" actionName="StudentHistory"/>
  </container>
  <!--学员管理权限配置结束-->
</collegeAuthor>
