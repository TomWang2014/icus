      </layout>
    </parameter>
    <parameter>
      <parameterName value="@jsonstr" />
      <dbType value="String" />
      <size value="100" />
      <layout type="ICusCRM.Web.Portal.Models.MyLayOut">
        <conversionPattern value = "%property{jsonstr}"/>
      </layout>
    </parameter>
  </appender>


</log4net>
